<table class="table">
  <thead>
  <tr>
    <th>
      #
    </th>
    <th>
      LeetCode ID
    </th>
    <th>
      Joining time
    </th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><%= u.leetcode_id %></td>
      <td><%= time_ago_in_words(u.created_at) %></td>
    </tr>
  <% end %>
  </tbody>
</table>